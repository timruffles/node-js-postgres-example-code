{
  "name": "code",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "db:create": "createuser rdbs_node; createdb rdbs_node -O rdbs_node && createdb rdbs_node_test -O rdbs_node",
    "db:destroy": "psql -c 'drop database \"rdbs_node\"'; psql -c 'drop database \"rdbs_node_test\"'",
    "db:migrate": "knex migrate:latest",
    "db:test:reset": "psql -c 'drop database \"rdbs_node_test\"' && createdb rdbs_node_test -O rdbs_node",
    "test": "npm run -s db:test:reset && mocha src/*.test.js",
    "test:ci": "npm run -s db:create && NODE_ENV=testing npm run -s db:migrate && mocha src/*.test.js",
    "seed-one": "node .util/run-seed 01_initial"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "knex": "^0.7.3",
    "mocha": "^2.1.0",
    "pg": "^4.2.0"
  },
  "devDepedencies": {
    "lodash": "*"
  }
}
